<% songs.forEach(function(song) { %>
    <div class="well">
      <h2><%= song['im:name'].label %></h2>
      <form method="POST" action="/favorites">
        <input hidden type="text" name="name" value="<%= song.name %>">
        <button class="btn btn-primary" type="submit">Add to Favorites</button>
      </form>
    </div>
  <% }); %>
</div>
